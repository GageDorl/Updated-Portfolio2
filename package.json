{
  "name": "updated-porfolio",
  "version": "0.1.0",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.0.2",
    "if-env": "^1.0.4",
    "nodemailer": "^6.4.2",
    "nodemon": "^2.0.2"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'updated-porfolio/*'\" \"npm run client\"",
    "client": "cd updated-porfolio && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd updated-porfolio && npm install",
    "build": "cd updated-porfolio && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://gagedorl.github.io/updated-portfolio2"
}
